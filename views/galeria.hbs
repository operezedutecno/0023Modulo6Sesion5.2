{{> header }}
<div class="container">
    <div class="row">
        {{#each fotos}}
        <div class="col-12 col-sm-6 col-md-4">
            <div class="card mt-3">
                <img src="{{ this.url }}" class="card-img-top" alt="{{ this.titulo }}">
                <div class="card-body">
                    <h5 class="card-title">{{ this.titulo }}</h5>
                    <p class="card-text">{{ this.contenido }}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>


{{> footer }}